{
  "name": "@packages/grpc",
  "version": "1.0.0",
  "description": "gRPC proto definitions and generated TypeScript code",
  "license": "MIT",
  "author": "",
  "private": true,
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm proto:generate && tsc",
    "proto:generate": "mkdir -p ./src/generated && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=nestJs=true --ts_proto_opt=outputIndex=true --ts_proto_opt=useDate=true --ts_proto_opt=unrecognizedEnum=false --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=outputServices=grpc-js --ts_proto_opt=esModuleInterop=true --proto_path=./protobufs ./protobufs/*.proto",
    "proto:watch": "chokidar \"protobufs/**/*.proto\" -c \"pnpm proto:generate\"",
    "clean": "rm -rf dist src/generated"
  },
  "dependencies": {
    "@bufbuild/protobuf": "catalog:",
    "@grpc/grpc-js": "catalog:",
    "@nestjs/microservices": "catalog:",
    "rxjs": "catalog:"
  },
  "devDependencies": {
    "@grpc/proto-loader": "catalog:",
    "@packages/tsconfig": "workspace:*",
    "chokidar-cli": "catalog:",
    "ts-proto": "catalog:",
    "typescript": "catalog:"
  }
}
