{
  "compilerOptions": {
    "target": "es2021",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "incremental": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noImplicitOverride": true
  }
}
