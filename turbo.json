{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "rest:dev": {
      "cache": false,
      "persistent": true
    },
    "graphql:dev": {
      "cache": false,
      "persistent": true
    },
    "grpc:dev": {
      "cache": false,
      "persistent": true
    },
    "migration:run": {
      "cache": false,
      "outputs": []
    },
    "migration:run:prod": {
      "cache": false,
      "outputs": []
    }
  }
}
